{
    "manifest_version": 2,
    "name": "Nothing Butt Trouble Popping Up",
    "version": "0.4",
     "content_scripts": [
        {
            "matches": ["<all_urls>"
            ],
            "js": ["jquery-3.4.1.min.js", "content.js"]
        }
    ],
    "browser_action": {
        "default_icon": "littlebutt.png"
    },

    "background": {
        "scripts": ["background.js"]
    },
    "permissions":[
      "<all_urls>",
      "activeTab",
      "tabs",
      "http://*/",
      "https://*/"
    ]
}